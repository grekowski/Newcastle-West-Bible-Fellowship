suPHP_ConfigPath /home/cjurdtcw/public_html

# supresses the possibilty of directly listing files in a directory where no index.php or index.html is set. 
Options All -Indexes

# Prevent viewing of .htaccess file
<Files .htaccess>
	order allow,deny
	deny from all
</Files>

RewriteEngine On
RewriteCond %{HTTP_HOST} ^ncwbiblefellowship.com
RewriteRule (.*) http://www.ncwbiblefellowship.com/$1 [R=301,L]

# Redirect index.php to domain
RewriteCond %{THE_REQUEST} ^.*/index\.php 
RewriteRule ^(.*)index.php$ /$1 [R=301,L]

